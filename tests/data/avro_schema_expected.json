            "type": "string"
        },
        {
            "name": "market",
            "type": "string"
        },
        {
            "name": "exchange",
            "type": "string"
        },
        {
            "name": "trade_correlation_id",
            "type": "string"
        },
        {
            "name": "direction",
            "type": {
                "type": "enum",
